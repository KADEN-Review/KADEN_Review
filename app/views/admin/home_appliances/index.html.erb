<div>
  <h1>家電一覧</h1>
  <div class="search-form">
    <%= form_with url: search_path, methot: :get, local: true do |f| %>
      <%= f.text_field :model_number, value: @home_applianc %>
      <%= f.submit "検索" %>
    <% end 
  </div>
  <div>
    <%= @home_appliances.each do |home_applianc| %>
      <% link_to admin_home_appliance_path do %>
        <%= home_applianc.model_number %>
      <% end %>
    <% end %>
  </div>
</div>